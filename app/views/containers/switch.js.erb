<% if @operate_success %>

$("input#<%= @container.container_hash %>").removeClass("disabled");
console.debug('input#<%= @container.container_hash %>');

<% if @turn == "on" # turn on %>
$("#card-<%= @container.container_hash %> code.port").html("<%= @container.port %>");// ? => num

var $qr_img = $('<img />');
$qr_img.attr("src", '/qr?str=' + "<%= @container.qr_url %>");
$("#card-<%= @container.container_hash %> div.qr").html($qr_img);
<% end %>


<% else %>

$("#container_cards").html("<%=j render("dashboard/cards") %>");
$("#modal_container").html("<%= j render("shared/modal_sample", object: @hint) %>");

<% end %>